{
	"name": "context-snippets",
	"displayName": "context-snippets",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.52.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:latex"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "context-snippets.image_figure",
				"title": "Latex Image figure",
				"category": "Latex Con"
			},
			{
				"command": "context-snippets.inkscape",
				"title": "Inkscape",
				"category": "Latex Con"
			}
		],
		"configuration": {
			"title": "Context snippets",
			"properties": {
				"context-snippets.inkscapeexe": {
					"type": "string",
					"description": "Inkscape absolute path",
					"scope": "resource",
					"default": "\"C:\\Program Files\\Inkscape\\bin\\inkscape.exe\""
				},
				"context-snippets.inkscapefolder": {
					"type": "string",
					"description": "Inkscape absolute path",
					"scope": "resource",
					"default": "inkscape"
				},
				"context-snippets.snippets": {
					"type": "array",
					"description": "List of snippets",
					"scope": "resource",
					"items": {
						"type": "object",
						"properties": {
							"prefix": {
								"type": "string",
								"default": "sqrt",
								"description": "Prefix, this is what you type to access the snippet"
							},
							"body": {
								"type": "string",
								"default": "\\sqrt($1)",
								"description": "What the snippet will be replaced with"
							},
							"description": {
								"type": "string",
								"default": null,
								"description": "Describe the snippet"
							}
						}
					},
					"default": [
						{
							"prefix": "lim",
							"body": "\\lim_{${1:x} \\to ${2:\\infty}} ${3:f(x)})",
							"description": "limit"
						},
						{
							"prefix": "sum",
							"body": "\\sum_{${1:n=1}}^{${2:\\infty}} ${3:f(n)}",
							"description": "sum"
						},
						{
							"prefix": "sqrt",
							"body": "\\sqrt{$1}",
							"description": "sqrt"
						},
						{
							"prefix": "nrt",
							"body": "\\sqrt[${1:2}]{$2}",
							"description": "sqrt"
						},
						{
							"prefix": "alpha",
							"body": "\\alpha"
						},
